{
	"name": "flashboard-demo",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "react-router build",
		"db:generate": "kysely-codegen --dialect postgres --out-file=./app/db/types.d.ts --camel-case",
		"db:migrate": "tsx ./app/db/migrate.ts && pnpm run db:generate",
		"db:migration": "tsx ./app/db/migration.ts",
		"db:rollback": "tsx ./app/db/rollback.ts && pnpm run db:generate",
		"dev": "react-router dev",
		"lint": "node_modules/.bin/biome check --error-on-warnings",
		"lint-fix": "node_modules/.bin/biome check --write --error-on-warnings",
		"start": "react-router-serve ./build/server/index.js",
		"tsc": "react-router typegen && tsc"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.782.0",
		"@aws-sdk/s3-request-presigner": "^3.782.0",
		"@headlessui/react": "^2.2.1",
		"@heroicons/react": "^2.2.0",
		"@react-router/node": "^7.5.0",
		"@react-router/serve": "^7.5.0",
		"dotenv": "^16.4.7",
		"isbot": "^5.1.25",
		"kysely": "^0.27.6",
		"kysely-codegen": "^0.18.3",
		"pg": "^8.14.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-router": "^7.5.0",
		"string-ts": "^2.2.1",
		"tsx": "^4.19.3",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@react-router/dev": "^7.5.0",
		"@tailwindcss/vite": "^4.1.3",
		"@types/node": "^22.14.0",
		"@types/pg": "^8.11.11",
		"@types/react": "^19.1.0",
		"@types/react-dom": "^19.1.1",
		"tailwindcss": "^4.1.3",
		"typescript": "^5.8.3",
		"vite": "^5.4.11",
		"vite-tsconfig-paths": "^5.1.4"
	}
}
